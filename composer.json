    "keywords": ["laravel", "blade", "router", "standalone", "templating", "illuminate"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Frankie Wittevrongel",
            "email": "frankie@cupoftea.io"
        }
    ],
    "require": {
